{
    "name": "donovanbroquin/flysystem-alfresco",
    "description": "Flysystem driver using Alfresco API",
    "homepage": "https://github.com/donovanbroquin/flysystem-alfresco",
    "keywords": [
        "flysystem-alfresco",
        "flysystem",
        "alfresco"
    ],
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "league/flysystem": "^3.7.0",
        "nesbot/carbon": "^3.7",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.35",
        "laravel/pint": "^1.17",
        "rector/rector": "*",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Donovanbroquin\\FlysystemAlfresco\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Donovan Broquin",
            "email": "donovan@broquin.dev"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
